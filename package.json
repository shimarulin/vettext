{
  "name": "vuettext",
  "description": "Gettext tools to improve your internationalization workflow with Vue-i18n",
  "version": "0.5.1",
  "author": "Vyacheslav Shimarulin <shimarulin@gmail.com>",
  "bin": {
    "vuettext": "./bin/run"
  },
  "bugs": "https://github.com/shimarulin/vuettext/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/inquirer": "^0.0.43",
    "@types/parse5": "^5.0.0",
    "acorn": "^6.0.4",
    "acorn-dynamic-import": "^4.0.0",
    "fast-glob": "^2.2.4",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "iso-639-3": "^1.1.0",
    "moment": "^2.23.0",
    "parse5": "^5.1.0",
    "pofile": "^1.0.11",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/debug": "^0.0.31",
    "@types/del": "^3.0.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "conventional-changelog-cli": "^2.0.11",
    "debug": "^4.1.0",
    "del": "^3.0.0",
    "fancy-test": "^1.4.1",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/shimarulin/vuettext",
  "keywords": [
    "vue",
    "vue-i18n",
    "gettext"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vuettext",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shimarulin/vuettext.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "TS_NODE_FILES=true nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md && oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
